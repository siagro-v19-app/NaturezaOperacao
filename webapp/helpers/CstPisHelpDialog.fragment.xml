<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="CST - PIS"
		class="sapUiPopupWithPadding sapUiSizeCompact"
		items="{/CstPiss}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{Descricao}"
			description="{Codigo}"/>
	</SelectDialog>
</core:FragmentDefinition>